{
    "name": "wibe-graphql-schema",
    "version": "0.0.1",
    "description": "wibe-graphql-schema",
    "scripts": {
        "generate:types": "dts-bundle-generator src/index.ts -o lib/index.d.ts",
        "build": "bun build src/index.ts --minify --target=bun --outdir lib && bun run generate:types",
        "test": "bun test --watch",
        "linter": "biome lint ./src/**/*.ts",
        "formatter": "biome format --write ./src"
    },
    "dependencies": {},
    "devDependencies": {
        "@biomejs/biome": "1.5.3",
        "@parcel/watcher": "2.3.0",
        "@types/bun": "1.0.2",
        "dts-bundle-generator": "8.0.1",
        "typescript": "5.3.3"
    }
}
